{
  "routes": [
    {
      "type": "application",
      "name": "@hyp/navbar",
      "props": {
        "navApps": [
          { "path": "/beauty", "text": "@hyp/beauty" },
          { "path": "/payments", "text": "@hyp/payments" }
        ]
      }
    },
    {
      "type": "route",
      "path": "/",
      "exact": true,
      "routes": [
        {
          "type": "application",
          "name": "@hyp/navbar"
        }
      ]
    },
    {
      "type": "route",
      "path": "beauty",
      "routes": [
        {
          "type": "application",
          "name": "@hyp/beauty"
        }
      ]
    },
    {
      "type": "route",
      "path": "payments",
      "routes": [
        {
          "type": "application",
          "name": "@hyp/payments"
        }
      ]
    }
  ]
}